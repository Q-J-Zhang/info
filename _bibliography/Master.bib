  isbn     = {0780373979},
  keywords = {AUV, Control, Robot, Swimming},
}

@Article{Guo2000,
  author   = {Guo, Shuxiang Guo Shuxiang and Sugimoto, K. and Hata, S. and Su, Jianliang Su Jianliang and Oguro, K.},
  journal  = {Proceedings. 2000 IEEE/RSJ International Conference on Intelligent Robots and Systems},
  title    = {{A new type of underwater fish-like microrobot}},
  year     = {2000},
  issn     = {1083-4435},
  number   = {1},
  pages    = {136--141},
  volume   = {2},
  abstract = {It is our purpose to develop an underwater microrobot that has the$\backslash$ncharacteristics of flexibility, driven by a low voltage, good response$\backslash$nand safe body. We have reported an underwater fish-like microrobot with$\backslash$n2 DOF that has the characteristics of flexibility, good response and$\backslash$nsafe body before. In this paper, we propose a new prototype model of an$\backslash$nunderwater microrobot utilizing an ICPF (ionic conducting polymer film)$\backslash$nactuator as the servo actuator to realize swimming motion with 3 DOF. A$\backslash$nbiomimetic fish-like microrobot using an ICPF actuator as a propulsion$\backslash$ntail fin and a buoyancy adjuster for a microrobot swimming structure in$\backslash$nwater or aqueous medium is developed. The overall size of the underwater$\backslash$nmicrorobot prototype shaped as a fish is 45 mm in length, 10 mm in width$\backslash$nand 4 mm in thickness. It has a pair of fins and a floatage adjuster.$\backslash$nCharacteristics of the underwater microrobot are measured by changing$\backslash$nthe frequency and amplitude of input voltage. The experimental results$\backslash$nindicate that the swimming speed of the proposed underwater microrobot$\backslash$ncan be controlled by changing the frequency of input voltage, the moving$\backslash$ndirection (upward or downward) can be controlled by changing the$\backslash$namplitude and the frequency of input voltage},
  doi      = {10.1109/IROS.2000.893128},
  file     = {:C$\backslash$:/Users/LabUser/University of Pittsburgh/OneDrive - University of Pittsburgh/Mag µBots References/Swimming/2003-A New Type of Fish-Like Underwater Microrobot.pdf:pdf},
