  groups     = {LabUser:6},
  keywords   = {Biomechanical Phenomena, Electric Stimulation, Humans, Isometric Contraction, Isometric Contraction: physiology, Knee Joint, Knee Joint: physiology, Models, Biological, Muscle Fatigue, Muscle Fatigue: physiology, Muscle, Skeletal, Skeletal: physiology},
  owner      = {LabUser},
