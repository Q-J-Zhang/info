  url        = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2615438{\&}tool=pmcentrez{\&}rendertype=abstract},
}

@Article{Perumal2002,
  author     = {Perumal, Ramu and Wexler, Anthony S and Ding, Jun and Binder-Macleod, Stuart a},
  journal    = {J. Biomech.},
  title      = {{Modeling the length dependence of isometric force in human quadriceps muscles.}},
  year       = {2002},
  issn       = {0021-9290},
